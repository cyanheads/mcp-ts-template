{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": true,
    "copyToClipboard": false
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      ".clinerules",
      "AGENTS.md",
      "repomix.config.json",
      "docs/api-references/"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
